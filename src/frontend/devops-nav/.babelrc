{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": "chrome > 66 and > 1%",
                "debug": false,
                "useBuiltIns": "usage"
            }
        ],
        "stage-2"
    ],
    "plugins": [ "transform-runtime", "transform-vue-jsx", "transform-class-properties", "transform-decorators-legacy"],
    "env": {
        "test": {
            "presets": ["env", "stage-1"],
            "plugins": ["istanbul"]
        }
    }
}
