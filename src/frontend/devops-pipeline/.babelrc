{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": "chrome > 66",
                "debug": false,
                "useBuiltIns": true
            }
        ],
        "stage-2"
    ],
    "plugins": ["transform-runtime", "transform-object-assign", "lodash", "syntax-dynamic-import", "transform-vue-jsx"],
    "env": {
        "test": {
            "presets": ["env", "stage-2"],
            "plugins": ["istanbul"]
        }
    }
}
